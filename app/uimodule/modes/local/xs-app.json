{
  "welcomeFile": "/app/",
  "authenticationMethod": "none", 
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "/catalog/(.*)$",
      "target": "/catalog/$1",
      "destination": "susaas-srv-api"
    },{
      "source": "^/app/(.*)$",
      "target": "$1",
      "localDir": "../../webapp"
    },{
      "source": "^/appconfig/(.*)",
      "target": "/appconfig/$1",
      "localDir": "../../webapp"
    }
  ]
}